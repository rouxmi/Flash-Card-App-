<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="435.0" prefWidth="671.0">
         <left>
            <ScrollPane prefHeight="403.0" prefWidth="414.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <VBox prefHeight="403.0" prefWidth="259.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#versEntrainement" text="Entrainement" />
                  <Button mnemonicParsing="false" text="Button" />
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </VBox>
         </right>
         <bottom>
            <Button mnemonicParsing="false" onAction="#versCreation" text="Ajouter une carte" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <ToolBar prefHeight="32.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <items>
                  <Button mnemonicParsing="false" onAction="#quitterAppli" text="Quitter" />
                  <Button mnemonicParsing="false" onAction="#allerAccueil" text="Accueil" />
               </items>
            </ToolBar>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
